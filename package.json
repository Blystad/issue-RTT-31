    "babel-preset-stage-1": "^6.1.18",
    "chai": "^3.4.1",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "1.0.0",
    "eslint-plugin-react": "^3.8.0",
    "karma": "^0.13.15",
    "karma-chai-sinon": "^0.1.5",
